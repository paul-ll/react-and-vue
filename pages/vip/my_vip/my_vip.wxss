@import "/utils/templates/no_data/no_data.wxss";
.vip-top {
    width: 100%;
    height: 322rpx;
}

.vip-top-main {
    width: 100%;
    height: 322rpx;
    position: relative;
    background: #FFB33D;
}

.vip-top-bg {
    width: 690rpx;
    height: 222rpx;
    margin: 0 auto;
    padding-top: 100rpx;
    position: relative;
}

.no_black_image {
    width: 690rpx;
    height: 222rpx;
    position: absolute;
    top: 100rpx;
    left: 0;
}

.vip_margin {
    padding-top: 1rpx;
}

button {
    background: none;
}

button::after {
    border: none;
}

button[type=warn] {
    color: #FF4891!important;
    background: #FFFFFF!important;
}

.vip_load {
    width: 144rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #FF4891;
    text-align: center;
    background: #FFFFFF;
    border: 1px solid #FF4891;
    border-radius: 2px;
    margin: 0 auto;
    margin-top: 200rpx;
}

.vip-top-user {
    position: relative;
    z-index: 1;
    display: flex;
    align-items: center;
    height: 112rpx;
    margin-top: 36rpx;
    margin-left: 60rpx;
}

.vip-top-img {
    width: 112rpx;
    height: 112rpx;
    margin-right: 28rpx;
    border-radius: 50%;
    position: relative;
}

.vip-top-header {
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
}

.vip-gold-img {
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
    position: absolute;
    bottom: 0;
    left: 0;
}

.vip-top-name {
    max-width: 230rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: PingFangSC-Semibold;
    font-size: 32rpx;
    color: #474245;
}

.vip-top-title, .vip-top-time {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #474245;
}
/* .vip-top-time{
    margin-right: 20rpx;
} */
.vip-top-text{
    display: flex;
}

.vip-top-button {
    width: 136rpx;
    height: 48rpx;
    border-radius: 8rpx;
    position: relative;
    margin-left: 16rpx;
}

.marginTop{
    margin-top: 25rpx;
}

.vip-top-kai {
    position: absolute;
    top: 0;
    left: 0;
    width: 136rpx;
    height: 48rpx;
}

.vip-top-right {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #474245;
    line-height: 48rpx;
    color: #ffffff;
    position: relative;
    text-align: center;
}

.vip-top-float {
    width: 624rpx;
    height: 104rpx;
    margin: 0 auto;
    transform: translate(0, -50%);
    -webkit-transform: translate(0, -50%);
}

.vip-float-img {
    position: absolute;
    top: 0;
    left: 0;
    width: 624rpx;
    height: 104rpx;
}

.vip-float-text {
    position: relative;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    line-height: 104rpx;
    margin-left: 52rpx;
    color: #474245;
    z-index: 1;
    display: flex;
}

.vip-float-red {
    color: #ff4891;
}

.vip-tab-flex {
    width: 690rpx;
    height: 180rpx;
    box-sizing: border-box;
    white-space: nowrap;
    overflow: auto;
    text-align: center;
    margin: 0 auto;
    margin-top: 56rpx;
}

.vip-tab-index {
    display: inline-block;
    width: 140rpx;
    margin: 0 auto;
    margin-top: 26rpx;
}

.vip-tab-img {
    width: 72rpx;
    height: 72rpx;
}

.vip-tab-text {
    max-width: 140rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #969696;
    letter-spacing: 0;
    margin-top: 16rpx;
}

.banners-swiper {
    width: 100%;
    /* height: 280rpx; */
}

.banners-image {
    width: 100%;
    /* height: 280rpx; */
}
.pagination {
    position: absolute;
    z-index: 1;
    bottom: 15rpx;
    right: 20rpx;
    background: rgba(0, 0, 0, .3);
    line-height: 1;
    padding: 8rpx 15rpx;
    border-radius: 24rpx;
    color: #fff;
    font-size: 24rpx;
}
.swipers .wx-swiper-dot {
    width: 10rpx;
    display: inline-flex;
    height: 10rpx;
    background: none;
    border: 1rpx solid rgba(255, 255, 255, 0.5);
    justify-content: space-between;
}

.swipers .wx-swiper-dot::before {
    width: 10rpx;
    height: 10rpx;
    content: '';
    flex-grow: 1;
    border-radius: 50%;
}

.swiper-box .wx-swiper-dot-active::before {
    background: rgba(255, 255, 255, 0.5);
}

.vip-group {
    width: 690rpx;
    box-sizing: border-box;
    white-space: nowrap;
    overflow: auto;
    text-align: center;
    margin: 0 auto;
    margin-top: 30rpx;
}

.vip-group-item {
    width: 380rpx;
    height: 160rpx;
    margin-right: 20rpx;
    display: inline-block;
}

.vip-group-image {
    width: 380rpx;
    height: 160rpx;
}

.dialog-container {
    transition: all 300ms;
    -webkit-transition: all 300ms;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
}

.activity_rule {
    width: 580rpx;
    height: 820rpx;
    border-radius: 20rpx;
    background: #FEFCEF;
}

.black-mask {
    background: rgba(0, 0, 0, .7);
    width: 100%;
    height: 100%;
}

.close-dialog-btn {
    position: absolute;
    width: 71rpx;
    height: 101rpx;
    top: -100rpx;
    right: 0;
    z-index: 2;
}

.dialog-body {
    width: 580rpx;
    height: 820rpx;
    text-align: center;
    position: relative;
    border-radius: 10px;
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 100;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
}

.dialog-body-vip {
    height: 940rpx;
    transform: translate(-50%, -45%);
    -webkit-transform: translate(-50%, -45%);
}

.dialog_older {
    width: 620rpx;
    height: 490rpx;
    text-align: center;
    position: relative;
    border-radius: 10px;
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 100;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
}

.dialog_body_img {
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 580rpx;
    height: 820rpx;
}

.dialog-img {
    width: 580rpx;
    height: 820rpx;
}

.transparent {
    opacity: 0;
}

.calc-img {
    width: 216rpx;
    height: 52rpx;
}

.calc-title {
    max-width: 520rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #474245;
    margin: 0 auto;
    margin-top: 36rpx;
}

.calc-text {
    font-family: PingFangSC-Semibold;
    font-size: 40rpx;
    color: #474245;
}

.calc-money {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    height: 80rpx;
    margin-top: 4rpx;
    vertical-align: text-bottom;
}

.calc-money-left {
    font-family: PingFangSC-Semibold;
    font-size: 40rpx;
    line-height: 40rpx;
    letter-spacing: 0;
    margin-bottom: 5rpx;
}

.calc-money-right {
    font-family: PingFangSC-Semibold;
    font-size: 64rpx;
    line-height: 64rpx;
    /* color: #FF4891; */
    letter-spacing: 0;
}

.calc-list {
    width: 520rpx;
    margin: 0 auto;
    margin-top: 30rpx;
}

.calc-list-flex {
    height: 90rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx dashed #E9D8B7;
}

.calc-list-left {
    text-align: left;
}

.calc-list-title {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #443E42;
    letter-spacing: 0;
}

.calc-list-info {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #969696;
    letter-spacing: 0;
}

.calc-list-right {
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #443E42;
    letter-spacing: 0;
}

.calc-open {
    width: 440rpx;
    height: 80rpx;
    background: #FF4891;
    border-radius: 4rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    line-height: 80rpx;
    color: #FFFFFF;
    text-align: center;
    position: absolute;
    bottom: 40rpx;
    left: 70rpx;
}

.open-img {
    width: 580rpx;
    height: 940rpx;
}

.dialog-body-group {
    width: 500rpx;
    height: 396rpx;
    text-align: center;
    background: #FFFFFF;
    border-radius: 16rpx;
}

.dialog-group-title {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #474346;
    margin-top: 34rpx;
}

.dialog-group-text {
    width: 336rpx;
    margin: 0 auto;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #474346;
    margin-top: 32rpx;
}

.dialog-group-flex {
    display: flex;
    justify-content: center;
    margin-top: 106rpx;
}

.dialog-group-right, .dialog-group-left {
    width: 206rpx;
    height: 80rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    color: #FFFFFF;
    border-radius: 4rpx;
}

.dialog-group-left {
    background: #C6C6C6;
    margin-right: 28rpx;
}

.dialog-group-right {
    background: #FF4891;
}

.click_button_right {
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    top: 100rpx;
    right: 0;
    z-index: 99;
}

.open-vip-button {
    position: absolute;
    left: 50%;
    bottom: 80rpx;
    width: 440rpx;
    height: 80rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    text-align: center;
    line-height: 80rpx;
    background: #FF4891;
    color: #ffffff;
    transform: translate(-50%, 0);
    -webkit-transform: translate(-50%, 0);
}

.fixed-tips {
    width: 100%;
    height: 96rpx;
    background: #E7AB21;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 99;
    /* transform: translate(-50%, 0);
    -webkit-transform: translate(-50%,0); */
}

.fixed-tips-img {
    width: 100%;
    height: 96rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.fixed-tips-main {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}

.fixed-tips-left {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    line-height: 96rpx;
    color: #FFFFFF;
    margin-right: 90rpx;
}

.fixed-tips-right {
    width: 128rpx;
    height: 50rpx;
    background: #FFFFFF;
    border-radius: 4rpx;
    text-align: center;
    line-height: 50rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #EBA62B;
}

.tips-hide {
    display: none;
}

.tips-show {
    display: block;
}

.vip-list-title {
    margin-top: 60rpx;
    margin-bottom: 12rpx;
    margin-left: 30rpx;
    font-family: PingFangSC-Semibold;
    font-size: 40rpx;
    color: #474245;
    letter-spacing: 0;
}

.blank-view {
    height: 63rpx;
    line-height: 63rpx;
    text-align: center;
    color: #969696;
    background: #fff;
    font-size: 32rpx;
}

.load-more-product {
    white-space: nowrap;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    color: #999;
    text-align: center;
    background: #fff;
}

.load-more-gif {
    display: inline-block;
    vertical-align: middle;
    width: 44rpx;
    height: 44rpx;
    animation: loading 800ms forwards infinite linear;
}

@keyframes loading {
    0% {
        transform: rotateZ(0);
        -webkit-transform: rotateZ(0);
    }
    50% {
        transform: rotateZ(180deg);
        -webkit-transform: rotateZ(180deg);
    }
    100% {
        transform: rotateZ(360deg);
        -webkit-transform: rotateZ(360deg);
    }
}

.load-more-text {
    display: inline-block;
    vertical-align: middle;
}

