/**distribution_city.wxss**/
page {
    background: #f5f5f5;
}

.distribution-city-info {
    margin: 0rpx 30rpx 30rpx 30rpx;
}
.distribution-city-item-title {
    margin-top:30rpx;
    margin-bottom:16rpx;
    font-size: 28rpx;
    color:#888888;
}
.distribution-city-item-info {
    border:0px solid;
}
.distribution-city-area {
    font-size:32rpx;
    color:#353535;
    height:72rpx;
    line-height:72rpx;
    text-align: center;
    width:25%;
    border:0px solid;
    display:inline-block;
    margin-top:20rpx;
}
.distribution-city-area-item {
    width:150rpx;
    border:0px solid;
    display:inline-block;
    background-color:#ffffff;
}