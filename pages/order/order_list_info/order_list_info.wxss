.order-detail-express-wraper {
    background: #fff;
    padding-top: 0.1px;
    margin-top: 20rpx;
}

.order-detail-express {
    margin: 30rpx 0 0 30rpx;
    box-sizing: border-box;
    border-left: 1px solid #c6c6c6;
    padding-top: 0.1px;
}

.order-detail-express-msg {
    padding: 0 30rpx 30rpx 0;
    margin: 30rpx 0 0 30rpx;
    font-size: 28rpx;
    color: #969696;
    position: relative;
}

.order-detail-express-msg:first-of-type {
    margin-top: 0;
}

.order-detail-express-msg:last-of-type {
    border: none;
}

.order-detail-express-msg-status {
    font-size: 28rpx;
    margin-bottom: 20rpx;
    line-height: 42rpx;
}

.order-detail-express-msg:first-of-type {
    color:#353535;
}

.order-detail-express-msg-time {
    color: #969696;
    font-size: 24rpx;
}
.order-spot {
    position: absolute;
    left: 0;
    top: 0;
    margin-left: -30rpx;
    transform: translate(-55%, -10%);
    width: 30rpx;
    height: 30rpx;
}