.count-down-wraper {
    background: #fff;
    /*text-align: center;*/
    position: relative;
}

.count-down {
    margin: 0 auto;
    width: 120px;
    height: 120px;
}

.count-down-dec {
    width: 560rpx;
    height: 46rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    padding: 0 20rpx;
    line-height: 46rpx;
    background:rgba(255,72,145,0.05);
    border-radius: 25px;
    font-size: 24rpx;
    color: #969696;
    text-align: center;
}

.count-down-dig {
    text-align: center;
    font-size: 40rpx;
    color: #353535;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -80%);
}

.count-down-dig-time {

}

.count-down-dig-dec {
    color: #969696;
    font-size: 24rpx;
}
.count-down-flex{
    display: flex;
    justify-content: center;
    margin: 0 auto;
    width: 600rpx;
    position: relative;
    padding-bottom: 20rpx;
}
.count-down-img{
    width: 46rpx;
    height: 46rpx;
    background: #fff;
    border-radius: 60%;
   position: absolute;
   left: -23rpx;
   top: -4rpx;
}
.count-down-image{
    width: 46rpx;
    height: 46rpx;
    margin-top: 5rpx;
}
.count-down-order{
    padding-bottom: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.count-down-info{
    font-family: PingFangSC-Regular;
    font-size: 20px;
    color: #262626;
    letter-spacing: 0;
}
.count-down-fail,.count-down-link{
    width: 200rpx;
    height: 60rpx;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    line-height: 60rpx;
    color: #262626;
    border: 1px solid #C6C6C6;
    border-radius: 2px;
}
.count-down-fail{
    margin-right: 30rpx;
    background:#fff;
    vertical-align:middle;
    border: none;
}
.count-down-info-img{
    width: 24rpx;
    height: 24rpx;
    transform: rotate(270deg);
}
